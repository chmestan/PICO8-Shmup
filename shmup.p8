--collisions

function col(a,b,al,ar,at,ab,bl,br,bt,bb)
 local a_l = a.x+al
 local a_r = a.x+ar
 local a_t = a.y+at
 local a_b = a.y+ab
  
 local b_l = b.x+bl
 local b_r = b.x+br	
 local b_t = b.y+bt
 local b_b = b.y+bb
 
 if a_t > b_b then return false end
 if a_b < b_t then return false end
 if a_l > b_r then return false end
 if a_r < b_l then return false end
 
 return true
 
end

function colcheck()

 local invtime = 60
 if inv <= 0 then
	 for enm in all(enemies) do
	  if col(enm,ship,enm.l,enm.r,enm.t,enm.b,0,7,0,7) then
	   lives-=1
	   sfx(5)
	   explosion(ship.x,ship.y,{13,2,8,12},15)
	   --del(enemies,enm)
	   enm.hp-=1
	   inv=invtime
	   return
	  end
	 end
	elseif inv > 0 then
	 inv -=1
 end

 for enm in all(enemies) do
  for bul in all(bullets) do
   if col(enm,bul,enm.l,enm.r,enm.t,enm.b,1,6,1,6) then
    enm.hp-=1
    enm.flash=3
    if enm.hp<=0 then
   	 explosion(enm.x,enm.y,{10,7,9},20)
    	sfx(4)
    	if enm.tp == 1 and globalt%2 == 0 then
    		divide(enm,5)
    	end
    	del(enemies,enm)
    	
    else
     sfx(6)
     shwave(bul.x+7,bul.y+4,2,6,7,1,10)
    end
    del(bullets,bul)
   end
  end
 end
 
end

function explosion(coordx,coordy,clrchoice,q_p)

 shwave(coordx,coordy,3,25,7,3,25)
 for i=1,30 do
 local expl = {
  x=coordx+(rnd()-0.5)*5,
  y=coordy+(rnd()-0.5)*5,
  r=flr(rnd(5)+3),
  clr=rnd(clrchoice),
  sx=(rnd()-0.5) *4,
  sy=(rnd()-0.5) *4,
  hp=15 }
 add(explosions,expl)
 end
 for i=1, q_p do
 	local p = 
	  {x=coordx+(rnd()-0.5)*5,
	  y=coordy+(rnd()-0.5)*5,
	  sx=(rnd()-0.5) *3,
	  sy=(rnd()-0.5) *3,
	  clr = rnd(clrchoice), 
	  hp=12}
	 add(part,p)
 end 

end

function shwave(shx,shy,r,tr,clr,spd,q_p)
 local wave=
  {x=shx,y=shy,r=r,tr=tr,clr=clr,spd=spd}
 add(shwaves, wave)
 for i=1, q_p do
 	local p = 
	  {x=shx+flr(rnd(20)-5),
	  y=shy+flr(rnd(20)-5),
	  sx=(rnd()-0.5) *3*spd,
	  sy=(rnd()-0.5) *3*spd,
	  clr = 7, 
	  hp=rnd(10)+3}
	 add(part,p)
 end 
 
end

function animexpl()
 for expl in all(explosions) do
  expl.x += expl.sx
	 expl.y += expl.sy
	 expl.sx*=0.85
	 expl.sy*=0.85
	 expl.hp-=1
	 expl.r=max(expl.r-0.5,1) 
	 if expl.hp <= 2 then
	  expl.clr = 1
	 elseif expl.hp <= 4 then
	  expl.clr = 2
	 
	 end
  if expl.hp<=0 then
   del(explosions,expl)
  end
 end
 for p in all(part) do
  p.hp-=1
  p.x += p.sx
	 p.y += p.sy
	 p.sx*=0.9
	 p.sy*=0.9
	 
  if p.hp<=0 then
   del(part,p)
  elseif p.hp <=1 then
   p.clr = 2
  elseif p.hp <= 3 then
   p.clr = 10
  end
 end
end

function expldraw()

 animexpl()
	for expl in all(explosions) do
	 circfill(expl.x,expl.y,expl.r,expl.clr)
	end
	
	for wave in all(shwaves) do
	 circ(wave.x,wave.y,wave.r,wave.clr)
	 wave.r+=wave.spd
	 if wave.r > wave.tr then
	  del(shwaves,wave)
	 elseif	wave.r >= 20 then
	  wave.clr = 1
	 elseif wave.r >= 15 then
	  wave.clr = 13
	 end
	end
	
	for p in all(part) do
	 pset(p.x,p.y,p.clr)
	end

end
