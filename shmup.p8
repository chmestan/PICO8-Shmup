-- init and startgame

function _init()

 mode = "start"
 blinkt=0
 transt = 0
 music(0)
 globalt=0
 lockout=0
 
end

function startgame()
 cls(0)
 
 music(2)
 
 wave=1
 wavetime=64
 
 ship=
  {x=20,y=64,
  sx=1,sy=2,
  spr=1,
  w=1,h=1}
  
 canshoot=false

	speedx=2
	speedy=2
 
 --anim
 flamespr=6
 muzzle=0

 maxlives = 5
 lives = maxlives

 t=0
 minutes = 0
 seconds = 0 
 
 starsinit()
 
 bullets={}
 
 enemies={}
 --enemiesinit()
 
 explosions={}
 part={}
 shwaves={}
 sparks={}
 
 timershoot=0
 inv=0
 
 mode = "wavetxt"
 
end
