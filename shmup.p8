-- stars

function starsinit()
 nbofstars=110
 clrchoice = {1,2,5,6,13}
 stars={}
 for i=1,nbofstars do
  local newstar={}
  newstar.x=flr(rnd(128))
  newstar.y=flr(rnd(128))
  newstar.clr=flr(rnd(clrchoice))
  newstar.spd=flr(rnd(4)+1)
  add(stars,newstar)
 end
end

function starfield()
 for star in all(stars) do
   if star.spd >= 4 then
    line(star.x,star.y,star.x-1,star.y,star.clr)
   else
    pset(star.x,star.y,star.clr)
   end
 end
end

function starmoving()
 for star in all(stars) do
   star.x -= star.spd
   if star.x < 0 then
    star.x = 128
    star.y=flr(rnd(128))
    star.clr=flr(rnd(clrchoice))
    star.spd=flr(rnd(4)+1)
   end
 end
end
