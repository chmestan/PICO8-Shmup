-- behaviour

function doenemy(enm)

	if enm.wait > 0 then
	 enm.wait -=1
	 return
	end

 if enm.mission == "flyin" then
  
  tween(enm,8)
	  
 elseif enm.mission == "hover" then
  
 elseif enm.mission == "attack" then
  targetplayer(enm)
 end
end

function pick(freq)
 
 if mode != "game" then
  return
 end
 
 if globalt%freq == flr(freq/2) then
 
	 local hovenm = {}
	 for enm in all(enemies) do
	  if enm.mission=="hover" then
	   add(hovenm,enm)
	  end
	 end 
 
  if #hovenm > 0 then
	  local enm = rnd(hovenm)
		 if enm.mission =="hover" then
		  enm.mission ="attack"
	 	end
 	end
 end
end

function tween(e,spd)
	e.x+=(e.posx-e.x)/spd
	e.y+=(e.posy-e.y)/spd
	  
  if abs(e.posx-e.x)<0.3 and 
   abs(e.posy-e.y)<0.3
   then
    e.mission = "hover"
  end
end

function targetplayer(enm)
 dirx = ship.x - enm.x
 diry = ship.y - enm.y

 magn = sqrt(dirx*dirx+diry*diry)
 normx = dirx / magn
 normy = diry / magn

 enm.x += normx * enm.spd
 enm.y += normy * enm.spd/2
end

function mvmt1(enm)
 local rndf = flr(rnd(2)+1)
 if globalt%rndf == 0 then	 
		enm.x += (rnd() - 0.5)*3
		enm.y += (rnd() - 0.5)*3
 end
end

function divide(enm,tp)
  spawnenemy(tp,enm.x,enm.y+15,0)
end
