-- updates

function _update()
 
 blinkt += 1
 globalt += 1
	if mode == "game" then
	 update_game()
	elseif mode == "start" then
	 update_start()
	elseif mode == "wavetxt" then
	 update_wavetxt()
	elseif mode == "over" then
	 update_over()
	elseif mode == "trans" then
	 update_trans()
	elseif mode == "win" then
	 update_win()
	end

end

function update_game()

 timershoot+=1


 input()
 shoot()

 animflame()
 muzzleupdt()
 animbullet()
 animenemies()

 timepassing()
 starmoving()
 
 colcheck()
 
 if lives <= 0 then
  mode = "over"
  lockout=globalt+30
  music(-1,500)
  music(1)
  return
 end
 
 if wave==1 then
  for enm in all(enemies) do
   if enm.mission == "attack" then
    mvmt1(enm)
   elseif enm.mission == "hover" then
    enm.mission = "attack"
   end 
  end
 end
 
 if mode=="game" and #enemies == 0 then 
	 nextwave()
	end
 
end

function update_start()
 if btn(5) == false then
  btnreleased = true
	end
	
	if btnreleased then
		if btnp(5) then
	  mode = "trans"
	  music(4)
	  btnreleased = false
	 end
	end
end

function update_over()
 if globalt<lockout then
  return
 end
 
 if btn(5) == false then
  btnreleased = true
	end
	
	if btnreleased then
		if btnp(5) then
	  mode = "start"
	  music(0)
	  btnreleased = false
	 end
	end
end

function update_trans()
 transt+=1
 if transt >= 60 then
  mode ="game"
  transt=0
  startgame()
 end
end

function update_wavetxt()
 update_game()
 wavetime-=1
 if wavetime <= 0 then
 	spawnwave(wave)
 	canshoot=true
  mode ="game"
 end
end

function update_win()
 if globalt<lockout then
  return
 end
	if btn(5) == false then
  btnreleased = true
	end
	
	if btnreleased then
		if btnp(5) then
	  mode = "start"
	  music(0)
	  btnreleased = false
	 end
	end
end
