-- draw

function _draw()

 if mode == "game" then
	 draw_game()
	elseif mode == "start" then
	 draw_start()
	elseif mode == "wavetxt" then
	 draw_wavetxt()
	elseif mode == "over" then
	 draw_over()
 elseif mode =="trans" then
  draw_trans()
 elseif	mode == "win" then
  draw_win()
	end

end

function draw_game()
	cls(0)
	
	starfield()
	
	if lives>0 then
		if inv <= 0 then
		 drawsprite(ship)
			spr(flamespr,ship.x-8,ship.y)
		else
		 if sin(t*4)<0.5 then
			 drawsprite(ship)
				spr(flamespr,ship.x-8,ship.y) 
	  end	
		end
	end
	
	for enm in all(enemies) do
		if enm.flash > 0 then 
		 enm.flash -=1
		 for i=1,15 do
		  pal(i,7)
		 end
		end
  drawsprite(enm)
  pal()
	end
	
	for blt in all(bullets) do
  drawsprite(blt)
	end

 muzzledraw()
 expldraw()
 
 uitimer()
 uilives()
 
end

function draw_start()
 cls(0)
 rect(16,16,112,112,8)
 for i=0,1 do
  for j=0,1 do
  circfill(16+96*i,16+96*j,4,0)
  circ(20+88*i,20+88*j,4,8)
  circfill(22+84*i,22+84*j,5,0)
  end
 end
 line(16,24,112,24,8)
 line(19,19,22,22,8)
 line(22,19,19,22,8)
 
 local txt = "press x key to start"
 print(txt,hcenter(txt),80,1)
end

function draw_over()
 draw_game()
 local txt1 = "game over!"
 local txt2 = "press x key to restart"
 print(txt1,hcenter(txt1),40,8)
 print(txt2,hcenter(txt2),80,blink({8,4,4,20,4,4}))
end

function draw_trans()
 local screenclr=8
 if transt < 32 then
	 for i=0,8 do
	   for j=0,8 do
	    circfill(i*16,j*16,transt,screenclr)
	   end
	 end
	 local txt = "ready?"
	 print (txt, hcenter(txt),64,0)
 elseif transt < 36 then
  cls(screenclr)
  local txt = "go!"
  print(txt,hcenter(txt),64,0)
 else
  for i=0,8 do
	   for j=0,8 do
	    circfill(i*16,j*16,transt-40,0)
	   end
	 end
 end
end

function draw_wavetxt()
 draw_game()
 local txt = "wave "..wave
 print(txt,hcenter(txt),40,blink({4,2,2,10,2,2}))
end

function draw_win()
 draw_game()
 local txt1 = "congratulations"
 local txt2 = "press x key to restart"
 print(txt1,hcenter(txt1),40,8)
 print(txt2,hcenter(txt2),80,blink({8,4,4,20,4,4}))

end
